<?xml version="1.0" encoding="utf-8"?>
<executable>
  <category>Filtering</category>
  <title>Extract Curves 2D (TubeTK)</title>
  <description>Extract the center-lines of tubular objects in an entire image for a given sigma.</description>
  <version>0.1.0</version>
  <documentation-url></documentation-url>
  <license></license>
  <contributor>Patrick Reynolds</contributor>
<acknowledgements>This work was funded by USC:EXPOSE.</acknowledgements>
  <parameters>
    <label>IO</label>
    <description>Input/output parameters</description>
    <image>
      <name>inputVolume</name>
      <label>Input Volume</label>
      <channel>input</channel>
      <index>0</index>
      <description>Input volume</description>
    </image>
    <image>
      <name>outputVolume</name>
      <label>Output Volume</label>
      <channel>output</channel>
      <index>1</index>
      <description>Output Volume</description>
    </image>
    <double>
      <name>sigma</name>
      <label>Sigma</label>
      <description>Sigma value</description>
      <longflag>sigma</longflag>
      <default>8.0</default>
    </double>
  </parameters>
</executable>

